{
  "name": "reclarker",
  "version": "1.0.0",
  "description": "e-commerce application that allows artists to upload, sell, and showcase their own artwork",
  "main": "index.js",
  "scripts": {
    "start": "node --trace-warnings backend/server/index.js",
    "server": "nodemon backend/server/index.js",
    "client": "npm run start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "keywords": [],
  "author": "Eric Humphries",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1072.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.7",
    "multer": "^1.4.4",
    "multer-s3": "^3.0.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.202.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "colors": "^1.4.0",
    "morgan": "^1.10.0"
  }
}
